<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module>

    <inherits name="org.gwtopenmaps.openlayers.OpenLayers" />
    <inherits name="com.google.gwt.json.JSON" />
    <inherits name="org.gwttime.Time" />
    <inherits name='com.google.web.bindery.requestfactory.RequestFactory' />
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="gwt.material.design.GwtMaterialWithJQuery" />
    <inherits name="gwt.material.design.addins.GwtMaterialAddins" />
    <inherits name="gwt.material.design.GwtMaterialDesignBasic" />
    <inherits name="org.fusesource.restygwt.RestyGWT"/>
    <inherits name="com.github.nmorel.gwtjackson.GwtJackson" />
    <inherits name="gwt.material.design.themes.ThemeBlueDebug"/>
    <inherits name="google.ggcharts"/>

    <inherits name="com.google.gwt.user.Debug"/>

    <set-property name="restygwt.encodeDecode.useGwtJackson" value="false" />



    <inherits name="org.moxieapps.gwt.highcharts.Highcharts"/>


    <!--
    <inherits name='GViz'/>
    -->
    <source path="client" />
    <source path="shared" />
    <source path="type" />

    <entry-point class="pmel.sdig.las.client.main.UI"/>
    <entry-point class="pmel.sdig.las.client.admin.Admin"/>
    <replace-with class="pmel.sdig.las.client.main.ClientFactoryImpl">
        <when-type-is class="pmel.sdig.las.client.main.ClientFactory" />
    </replace-with>


</module>
