<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:ma="urn:import:gwt.material.design.addins.client"
             xmlns:m='urn:import:gwt.material.design.client.ui'
             xmlns:las="urn:import:pmel.sdig.las.client.widget"
>
    <ui:style gss="true">
        body {
            background: #ffffff;
        }

        .pinright {
            margin-right: auto;
        }

        .datasetContainer {
            display: inline-flex;
            align-self: center;
        }
    </ui:style>
    <m:MaterialPanel>
        <m:MaterialDialog ui:field="loadDialog" dismissible="true">
            <m:MaterialDialogHeader></m:MaterialDialogHeader>
            <m:MaterialDialogContent ui:field="loadMessage"></m:MaterialDialogContent>
            <m:MaterialDialogFooter>
                <m:MaterialButton text="Cancel" ui:field="loadCancel"></m:MaterialButton>
            </m:MaterialDialogFooter>
        </m:MaterialDialog>
        <m:MaterialHeader ui:field="header">
            <m:MaterialNavBar activates="sidenav" ui:field="navbar" shadow="1">
                <m:MaterialNavSection ui:field="navSection" float="LEFT">
                    <m:MaterialButton ui:field="update" text="Update"></m:MaterialButton>

                    <m:MaterialButton text="Plot 1" enabled="false" iconType="ARROW_DROP_DOWN" iconPosition="RIGHT" textColor="WHITE" activates="plots-3" ui:field="plotsDropdownButton"/>
                    <m:MaterialDropDown activator="plots-3" constrainWidth="true" ui:field="plotsDropdown">
                        <m:MaterialLink title="Plot 1" text="Plot 1" separator="true"/>
                        <m:MaterialLink title="Plot 2" text="Plot 2" separator="true"/>
                        <m:MaterialLink title="Plot 4" text="Plot 4" separator="true"/>
                    </m:MaterialDropDown>

                    <m:MaterialLink ui:field="animate" text="Animate" waves="LIGHT" enabled="false"></m:MaterialLink>
                    <m:MaterialLink ui:field="correlationLink" text="Correlation Viewer" waves="LIGHT"
                                    enabled="false"></m:MaterialLink>
                    <m:MaterialLink ui:field="showValuesButton" text="Show values..." waves="LIGHT"
                                    enabled="false"></m:MaterialLink>
                    <m:MaterialLink ui:field="saveAsButton" text="Save as..." waves="LIGHT"
                                    enabled="false"></m:MaterialLink>
                </m:MaterialNavSection>
                <m:MaterialNavBrand ui:field="brand" text="LAS title will go here." overflow="HIDDEN"/>
                <m:MaterialLink ui:field="btnSearch" iconType="SEARCH" iconColor="WHITE" float="RIGHT"/>
            </m:MaterialNavBar>
            <!-- NavBar Search ... replaces entire navbar with a text field when searching. -->
            <m:MaterialNavBar ui:field="navBarSearch" width="100%" visible="false">
                <m:MaterialSearch ui:field="txtSearch" placeholder="search terms e.g. temperature" backgroundColor="WHITE" active="true" grid="s12" shadow="1"/>
            </m:MaterialNavBar>
        </m:MaterialHeader>
        <m:MaterialSideNavPush m:id="sidenav" width="400" ui:field="sideNav" withHeader="true" depth="0">
            <m:MaterialSideNavContent padding="0">
                <!--<m:MaterialNavBrand>-->
                    <!---->
                <!--</m:MaterialNavBrand>-->
                <!--<m:MaterialRow marginBottom="0" lineHeight="20">-->
                    <!--<m:MaterialColumn height="32" grid="s4">-->
                        <!--<m:MaterialLink ui:field="total" height="32" textColor="BLUE" text="Data Sets"></m:MaterialLink>-->
                    <!--</m:MaterialColumn>-->
                    <!--<m:MaterialColumn height="32" grid="s4">-->
                        <!--<m:MaterialLink ui:field="grids" height="32" textColor="BLUE" text="Grids"></m:MaterialLink>-->
                    <!--</m:MaterialColumn>-->
                    <!--<m:MaterialColumn height="32" grid="s4">-->
                        <!--<m:MaterialLink ui:field="discrete" height="32" textColor="BLUE" text="Discrete"></m:MaterialLink>-->
                    <!--</m:MaterialColumn>-->
                <!--</m:MaterialRow>-->
                <m:MaterialCollapsible ui:field="navcollapsible" accordion="false">
                    <m:MaterialCollapsibleItem ui:field="dataItem">
                        <m:MaterialCollapsibleHeader addStyleNames="LAS-accordian">
                            <m:MaterialPanel addStyleNames="valign-wrapper">
                                <m:MaterialIcon marginLeft="4" ui:field="home" iconType="HOME"></m:MaterialIcon>
                                <m:MaterialLink fontWeight="BOLDER" fontSize="1.4em" text="Data" display="FLEX"></m:MaterialLink>
                                <m:MaterialLink ui:field="browse" fontSize="1.2em" text="(Browse)" display="FLEX"></m:MaterialLink>
                                <m:MaterialIcon ui:field="back" iconType="ARROW_BACK" iconPosition="RIGHT"/>
                            </m:MaterialPanel>
                            <m:MaterialProgress ui:field="dataProgress" type="INDETERMINATE"
                                                display="NONE"></m:MaterialProgress>
                        </m:MaterialCollapsibleHeader>
                        <m:MaterialCollapsibleBody padding="0">
                            <m:MaterialCollection ui:field="datasets">
                                <m:MaterialCollectionItem>
                                    <m:MaterialPanel addStyleNames="valign-wrapper">
                                        <m:MaterialLink marginLeft="8" display="FLEX"
                                                        text="This is a Data Set Title that is rediculously long compare to the space available for it's display"></m:MaterialLink>
                                        <m:MaterialIcon iconPosition="RIGHT" iconType="INFO" display="FLEX"
                                                        marginRight="4"></m:MaterialIcon>
                                    </m:MaterialPanel>
                                </m:MaterialCollectionItem>
                                <m:MaterialCollectionItem>
                                    <m:MaterialPanel addStyleNames="valign-wrapper">
                                        <m:MaterialLink marginLeft="8" display="FLEX"
                                                        text="Short Name"></m:MaterialLink>
                                        <m:MaterialIcon iconPosition="RIGHT" iconType="INFO" display="FLEX"
                                                        marginRight="4"></m:MaterialIcon>
                                    </m:MaterialPanel>
                                </m:MaterialCollectionItem>
                                <m:MaterialCollectionItem>
                                    <m:MaterialPanel addStyleNames="valign-wrapper">
                                        <m:MaterialLink marginLeft="8" display="FLEX" text="ALL CAPS"></m:MaterialLink>
                                        <m:MaterialIcon iconPosition="RIGHT" iconType="INFO" display="FLEX"
                                                        marginRight="4"></m:MaterialIcon>
                                    </m:MaterialPanel>
                                </m:MaterialCollectionItem>
                            </m:MaterialCollection>
                        </m:MaterialCollapsibleBody>
                    </m:MaterialCollapsibleItem>
                    <m:MaterialCollapsibleItem>
                        <m:MaterialCollapsibleHeader>
                            <m:MaterialLink fontWeight="BOLDER" fontSize="1.4em" text="Axes Selections"></m:MaterialLink>
                        </m:MaterialCollapsibleHeader>
                        <m:MaterialCollapsibleBody padding="0">
                            <m:MaterialPanel m:id="timeRegion" padding="2" width="375px">
                                <m:MaterialRow grid="s12" margin="0">
                                    <m:MaterialPanel padding="8" ui:field="mapPanel"></m:MaterialPanel>
                                </m:MaterialRow>
                                <m:MaterialRow grid="s12" marginBottom="0">
                                    <m:MaterialPanel padding="8" ui:field="dateTimePanel" width="375px"/>
                                </m:MaterialRow>
                                <m:MaterialRow grid="s12" marginBottom="0">
                                    <m:MaterialPanel margin="0" padding="8" ui:field="zaxisPanel" grid="s12"/>
                                </m:MaterialRow>
                            </m:MaterialPanel>
                        </m:MaterialCollapsibleBody>
                    </m:MaterialCollapsibleItem>
                    <m:MaterialCollapsibleItem>
                        <m:MaterialCollapsibleHeader>
                            <m:MaterialLink fontWeight="BOLDER" fontSize="1.4em" text="Plot Types"></m:MaterialLink>
                        </m:MaterialCollapsibleHeader>
                        <m:MaterialCollapsibleBody padding="0">
                            <m:MaterialPanel ui:field="products" textAlign="LEFT" overflow="AUTO">
                                <m:MaterialLabel
                                        text="Pick a data set and variable and plot choices will appear here."/>
                            </m:MaterialPanel>
                        </m:MaterialCollapsibleBody>
                    </m:MaterialCollapsibleItem>
                    <m:MaterialCollapsibleItem>
                        <m:MaterialCollapsibleHeader>
                            <m:MaterialLink fontWeight="BOLDER" fontSize="1.4em" text="Analysis Transformations"></m:MaterialLink>
                        </m:MaterialCollapsibleHeader>
                        <m:MaterialCollapsibleBody padding="0">
                            <m:MaterialPanel marginBottom="16" textAlign="LEFT" overflow="AUTO">
                                <m:MaterialLabel marginLeft="16" fontWeight="BOLD" textColor="BLUE"
                                                 text="Do Analysis"></m:MaterialLabel>
                                <!--
                                <m:MaterialSwitch helperText="Does this fill the Label?" verticalAlign="MIDDLE" ui:field="analysisSwitch" display="INLINE" paddingLeft="8" paddingRight="8" title="Activate a data reduction calculation." onLabel="Analysis On" offLabel="Analysis Off" enabled="true" value="false" />
                                -->
                                <m:MaterialSwitch marginLeft="32" ui:field="analysisSwitch" onLabel="On" offLabel="Off"
                                                  enabled="true" value="false"/>

                                <m:MaterialRow>
                                    <m:MaterialColumn grid="s6">
                                        <m:MaterialLabel text="Calculate"></m:MaterialLabel>
                                        <m:MaterialListBox ui:field="analysisListBox" old="true">
                                            <m:html.Option text="Average" value="Average"/>
                                            <m:html.Option text="Minimum" value="Minimum"/>
                                            <m:html.Option text="Maximum" value="Maximum"/>
                                            <m:html.Option text="Sum" value="Sum"/>
                                            <m:html.Option text="Variance" value="Variance"/>
                                        </m:MaterialListBox>
                                    </m:MaterialColumn>
                                    <m:MaterialColumn grid="s6">
                                        <m:MaterialLabel text="Over"></m:MaterialLabel>
                                        <m:MaterialListBox ui:field="overListBox" old="true">
                                            <m:html.Option text="Area" value="Area"/>
                                            <m:html.Option text="Longitude" value="Longitude"/>
                                            <m:html.Option text="Latitude" value="Latitude"/>
                                            <m:html.Option text="Height/Depth" value="Z"/>
                                            <m:html.Option text="Time" value="Time"/>
                                        </m:MaterialListBox>
                                    </m:MaterialColumn>
                                </m:MaterialRow>
                            </m:MaterialPanel>
                        </m:MaterialCollapsibleBody>
                    </m:MaterialCollapsibleItem>
                    <m:MaterialCollapsibleItem>
                        <m:MaterialCollapsibleHeader>
                            <m:MaterialLink fontWeight="BOLDER" fontSize="1.4em" text="Plot Options"></m:MaterialLink>
                        </m:MaterialCollapsibleHeader>
                        <m:MaterialCollapsibleBody padding="0">

                            <m:MaterialPanel ui:field="options" textAlign="LEFT">
                                <m:MaterialLabel text="Other options to modify the plot will appear here."/>
                            </m:MaterialPanel>

                        </m:MaterialCollapsibleBody>
                    </m:MaterialCollapsibleItem>
                </m:MaterialCollapsible>
            </m:MaterialSideNavContent>
        </m:MaterialSideNavPush>
        <m:MaterialContainer ui:field="main">
            <m:MaterialRow paddingTop="60" ui:field="outputRow01">

                <m:MaterialPanel ui:field="infoPanel" paddingTop="20">

                </m:MaterialPanel>

                <las:ResultsPanel ui:field="panel1" title="panel01"></las:ResultsPanel>
                <las:ComparePanel ui:field="panel2" title="panel02" grid="s6" visible="false"></las:ComparePanel>

            </m:MaterialRow>
            <m:MaterialRow ui:field="outputRow02">
                <m:MaterialColumn ui:field="panel3" grid="s5" offset="s2" paddingLeft="2" paddingRight="0"
                                  visibility="HIDDEN">
                    <m:MaterialCollapsible>
                        <m:MaterialCollapsibleItem>
                            <m:MaterialCollapsibleHeader paddingLeft="2">
                                <m:MaterialIcon iconType="SETTINGS" marginLeft="-4" marginRight="2"
                                                iconPosition="LEFT"></m:MaterialIcon>
                                <m:MaterialIcon iconType="ADD" iconPosition="RIGHT"></m:MaterialIcon>
                            </m:MaterialCollapsibleHeader>
                            <m:MaterialCollapsibleBody>
                                <m:MaterialLabel text="Fetching annotations..."></m:MaterialLabel>
                            </m:MaterialCollapsibleBody>
                        </m:MaterialCollapsibleItem>
                    </m:MaterialCollapsible>
                    <m:MaterialImage url="http://i.imgur.com/7JpG0F4.gif"/>
                </m:MaterialColumn>
                <m:MaterialColumn ui:field="panel4" paddingLeft="2" paddingRight="2" grid="s5" visibility="HIDDEN">
                    <m:MaterialCollapsible>
                        <m:MaterialCollapsibleItem>
                            <m:MaterialCollapsibleHeader paddingLeft="2">
                                <m:MaterialIcon iconType="SETTINGS" iconColor="BLUE" marginLeft="-4" marginRight="2"
                                                iconPosition="LEFT"></m:MaterialIcon>
                                <m:MaterialIcon iconType="ADD" iconPosition="RIGHT"></m:MaterialIcon>
                            </m:MaterialCollapsibleHeader>
                            <m:MaterialCollapsibleBody>
                                <m:MaterialLabel text="Fetching annotations..."></m:MaterialLabel>
                            </m:MaterialCollapsibleBody>
                        </m:MaterialCollapsibleItem>
                    </m:MaterialCollapsible>
                    <m:MaterialImage url="http://i.imgur.com/7JpG0F4.gif"/>
                </m:MaterialColumn>
            </m:MaterialRow>
        </m:MaterialContainer>
        <ma:window.MaterialWindow ui:field="animateWindow" title="Animate">
            <m:MaterialRow>
                <m:MaterialProgress ui:field="animateProgress" type="INDETERMINATE" display="NONE"></m:MaterialProgress>
            </m:MaterialRow>
            <m:MaterialRow>
                <m:MaterialColumn grid="s3">
                    <m:MaterialPanel padding="2" overflow="HIDDEN" width="375px">
                        <m:MaterialPanel padding="8" ui:field="animationDateTimePanel" width="375px"
                                         grid="s12"></m:MaterialPanel>
                        <m:MaterialPanel padding="8" ui:field="submitPanel" width="375px" grid="s12">
                            <m:MaterialLabel ui:field="frameCount" fontSize="1.8em" text="0 frames downloaded."/>
                            <m:MaterialLabel ui:field="animateHelp" marginTop="16"
                                             text="Select a time range, the number of time steps to skip between frames, and submit"></m:MaterialLabel>
                            <m:MaterialTextBox ui:field="time_step" value="1"></m:MaterialTextBox>
                            <m:MaterialButton ui:field="animateSubmit" text="Submit"></m:MaterialButton>
                        </m:MaterialPanel>
                        <m:MaterialPanel padding="8" ui:field="animationControls" display="NONE" width="375px"
                                         grid="s12">
                            <m:MaterialRow>
                                <m:MaterialLabel ui:field="speedLabel" text="1.5 seconds between images"/>
                                <m:MaterialRange ui:field="flipSpeed" min="200" max="3000"
                                                 value="1500"></m:MaterialRange>
                            </m:MaterialRow>
                            <m:MaterialRow>
                                <m:MaterialButton ui:field="stop" text="Stop"></m:MaterialButton>
                            </m:MaterialRow>
                            <m:MaterialRow ui:field="prevnext" display="NONE">
                                <m:MaterialButton ui:field="prev" text="Prev"></m:MaterialButton>
                                <m:MaterialButton ui:field="next" text="Next"></m:MaterialButton>
                            </m:MaterialRow>
                        </m:MaterialPanel>
                    </m:MaterialPanel>
                </m:MaterialColumn>
                <m:MaterialColumn grid="s9">
                    <las:ImagePanel ui:field="panel5"></las:ImagePanel>
                </m:MaterialColumn>
            </m:MaterialRow>
        </ma:window.MaterialWindow>
        <ma:window.MaterialWindow ui:field="showValuesWindow" title="Values">
        </ma:window.MaterialWindow>
        <ma:window.MaterialWindow ui:field="downloadWindow" title="Data Download">
            <m:MaterialPanel paddingTop="16">
                <m:MaterialRow>
                    <m:MaterialColumn grid="s4">
                        <m:MaterialLabel paddingTop="8" paddingBottom="8"
                                         text="Select the variables to include."></m:MaterialLabel>
                        <m:MaterialCollection marginTop="4" marginBottom="4" marginLeft="4" marginRight="4"
                                              lineHeight="32" ui:field="downloadDatasets"/>
                    </m:MaterialColumn>
                    <m:MaterialColumn grid="s3">
                        <m:MaterialLabel paddingTop="8" paddingBottom="8"
                                         text="Select the file format."></m:MaterialLabel>
                        <m:MaterialButton ui:field="formatsButton" text="netCDF" activates="formats"
                                          iconType="ARROW_DROP_DOWN" iconPosition="RIGHT"></m:MaterialButton>
                        <m:MaterialDropDown ui:field="formatsDropDown" activator="formats">
                            <m:MaterialLink text="netCDF"/>
                            <m:MaterialLink text="ASCII"/>
                            <m:MaterialLink text="CSV"/>
                        </m:MaterialDropDown>
                        <m:MaterialLabel paddingTop="8" paddingBottom="8"
                                         text="The range will match the plot. Choose the range of the axes orthogonal to the plot."></m:MaterialLabel>
                        <m:MaterialPanel padding="2" overflow="HIDDEN" width="375px">
                            <m:MaterialPanel padding="8" ui:field="downloadMapPanel"></m:MaterialPanel>
                            <m:MaterialPanel padding="8" ui:field="downloadDateTimePanel" width="375px"
                                             grid="s12"></m:MaterialPanel>
                            <m:MaterialPanel padding="8" ui:field="downloadZaxisPanel" grid="s12"></m:MaterialPanel>
                        </m:MaterialPanel>
                    </m:MaterialColumn>
                    <m:MaterialColumn grid="s3">
                        <m:MaterialButton ui:field="downloadButton" text="Request..."></m:MaterialButton>
                        <m:MaterialRow paddingTop="16">
                            <m:MaterialPreLoader ui:field="downloadLoader" display="NONE" size="SMALL">
                                <m:MaterialSpinner></m:MaterialSpinner>
                            </m:MaterialPreLoader>
                            <m:MaterialLink ui:field="downloadLink" text="" display="NONE"></m:MaterialLink>
                        </m:MaterialRow>
                        <m:MaterialLabel ui:field="downloadError" textColor="RED" text=""></m:MaterialLabel>
                    </m:MaterialColumn>
                </m:MaterialRow>
            </m:MaterialPanel>
        </ma:window.MaterialWindow>
        <ma:window.MaterialWindow ui:field="correlationWindow" title="Property-Property Plots">
            <m:MaterialRow>
                <m:MaterialProgress ui:field="correlationProgress" type="INDETERMINATE"
                                    display="NONE"></m:MaterialProgress>
            </m:MaterialRow>
            <m:MaterialPanel paddingTop="16">
                <m:MaterialRow>
                    <m:MaterialColumn grid="s3">
                        <m:MaterialRow>
                            <m:MaterialButton ui:field="correlationUpdate" text="Update"></m:MaterialButton>
                        </m:MaterialRow>
                        <m:MaterialRow>
                            <m:MaterialLabel text="Plot"></m:MaterialLabel>
                            <m:MaterialListBox ui:field="yVariableListBox"></m:MaterialListBox>
                        </m:MaterialRow>
                        <m:MaterialRow>
                            <m:MaterialLabel text="as a function of"></m:MaterialLabel>
                            <m:MaterialListBox ui:field="xVariableListBox"></m:MaterialListBox>
                        </m:MaterialRow>
                        <m:MaterialRow>
                            <m:MaterialLabel text="colored by"></m:MaterialLabel>
                            <m:MaterialRow>
                                <m:MaterialColumn grid="s2">
                                    <m:MaterialSwitch paddingTop="16" ui:field="colorByOn"
                                                      display="INLINE_BLOCK"></m:MaterialSwitch>
                                </m:MaterialColumn>
                                <m:MaterialColumn grid="s10">
                                    <m:MaterialListBox ui:field="cVariableListBox"></m:MaterialListBox>
                                </m:MaterialColumn>
                            </m:MaterialRow>
                        </m:MaterialRow>
                        <m:MaterialRow>
                            <m:MaterialCollapsible accordion="true">
                                <m:MaterialCollapsibleItem ui:field="correlationAxisItem">
                                    <m:MaterialCollapsibleHeader>
                                        <m:MaterialLink display="INLINE" paddingTop="16" fontSize="1.4em"
                                                        fontWeight="BOLDER" text="Axes Ranges"></m:MaterialLink>
                                    </m:MaterialCollapsibleHeader>
                                    <m:MaterialCollapsibleBody padding="0">
                                        <m:MaterialRow>
                                            <m:MaterialPanel padding="8" ui:field="correlationMapPanel"
                                                             grid="s12"></m:MaterialPanel>
                                        </m:MaterialRow>
                                        <m:MaterialRow>
                                            <m:MaterialPanel padding="8" ui:field="correlationDateTimePanel"
                                                             width="375px" grid="s12"></m:MaterialPanel>
                                        </m:MaterialRow>
                                        <m:MaterialRow>
                                            <m:MaterialPanel padding="8" ui:field="correlationZaxisPanel"
                                                             grid="s12"></m:MaterialPanel>
                                        </m:MaterialRow>
                                    </m:MaterialCollapsibleBody>
                                </m:MaterialCollapsibleItem>
                                <m:MaterialCollapsibleItem ui:field="correlationConstraintsItem">
                                    <m:MaterialCollapsibleHeader>
                                        <m:MaterialLink display="INLINE" paddingTop="16" fontSize="1.4em"
                                                        fontWeight="BOLDER"
                                                        text="Variable Constraints"></m:MaterialLink>
                                    </m:MaterialCollapsibleHeader>
                                    <m:MaterialCollapsibleBody padding="0">
                                        <m:MaterialPanel ui:field="plotConstraints">
                                            <m:MaterialPanel marginLeft="12" marginRight="12" border="2">
                                                <m:MaterialRow paddingTop="8" paddingBottom="8">
                                                    <m:MaterialLabel fontSize="1.4em"
                                                                     text="Add a Variable Constraint"></m:MaterialLabel>
                                                    <m:MaterialListBox
                                                            ui:field="variableConstraintListBox"></m:MaterialListBox>
                                                </m:MaterialRow>
                                            </m:MaterialPanel>
                                            <las:VariableConstraintWidget ui:field="xVariableConstraint"
                                                                          name="x var"></las:VariableConstraintWidget>
                                            <las:VariableConstraintWidget ui:field="yVariableConstraint"
                                                                          name="y var"></las:VariableConstraintWidget>
                                        </m:MaterialPanel>
                                        <m:MaterialPanel ui:field="variableConstraints">

                                        </m:MaterialPanel>
                                    </m:MaterialCollapsibleBody>
                                </m:MaterialCollapsibleItem>
                            </m:MaterialCollapsible>
                        </m:MaterialRow>
                    </m:MaterialColumn>
                    <m:MaterialColumn grid="s8">
                        <las:ResultsPanel ui:field="panel8" title="panel08"></las:ResultsPanel>
                    </m:MaterialColumn>
                </m:MaterialRow>
            </m:MaterialPanel>
        </ma:window.MaterialWindow>
        <m:MaterialFooter type="FIXED">
            <m:MaterialLink marginLeft="-380" paddingBottom="15" text="NOAA" href="http://www.noaa.gov/"
                            target="_blank"></m:MaterialLink>
            <m:MaterialLabel display="INLINE" text="|" paddingLeft="8" paddingRight="8"></m:MaterialLabel>
            <m:MaterialLink href="http://www.pmel.noaa.gov/" text="PMEL" target="_blank"></m:MaterialLink>
            <m:MaterialLabel display="INLINE" text="|" paddingLeft="8" paddingRight="8"></m:MaterialLabel>
            <m:MaterialLink href="http://www.pmel.noaa.gov/" text="Privacy" target="_blank"></m:MaterialLink>
            <m:MaterialLabel display="INLINE" text="|" paddingLeft="8" paddingRight="8"></m:MaterialLabel>
            <m:MaterialLink href="mailto:roland.schweitzer@noaa.gov" text="Contact Administrator"></m:MaterialLink>
        </m:MaterialFooter>
    </m:MaterialPanel>

</ui:UiBinder>