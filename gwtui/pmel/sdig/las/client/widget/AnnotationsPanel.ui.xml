<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
             xmlns:b3="urn:import:org.gwtbootstrap3.extras.slider.client.ui"
             xmlns:las="urn:import:pmel.sdig.las.client.widget">
    <ui:style>
        .ellipsis {
            white-space: nowrap;
            overflow: hidden;
            text-overflow: ellipsis;
        }
    </ui:style>
    <g:HTMLPanel>
        <b:PanelGroup b:id="annotations">
            <b:Panel type="INFO">
                <b:PanelHeader paddingBottom="1">
                      <b:Anchor paddingRight="8" paddingTop="10" iconBordered="true" pull="LEFT" ui:field="annotationsToggle" iconSize="LARGE" icon="CARET_DOWN" dataToggle="COLLAPSE" dataParent="#annotations" dataTarget="#annotationsCollapse"/>

                    <b:Breadcrumbs ui:field="breadcrumbs" addStyleNames="{style.ellipsis}">
                        <las:Breadcrumb ui:field="home" icon="HOME" text="Home"></las:Breadcrumb>
                    </b:Breadcrumbs>
                </b:PanelHeader>
                <b:PanelCollapse b:id="annotationsCollapse" ui:field="annotationsCollapse">
                    <b:PanelBody ui:field="annotationsPanelBody">

                    </b:PanelBody>
                </b:PanelCollapse>
            </b:Panel>
        </b:PanelGroup>
    </g:HTMLPanel>
</ui:UiBinder>