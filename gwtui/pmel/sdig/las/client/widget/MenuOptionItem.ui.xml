<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:m='urn:import:gwt.material.design.client.ui'
             xmlns:m.html='urn:import:gwt.material.design.client.ui.html'>
    <m:MaterialPanel lineHeight="32">
        <m:MaterialRow lineHeight="32" marginTop="0" marginBottom="0">
            <m:MaterialColumn lineHeight="32" grid="s12" margin="0">
                <m:MaterialCollapsible lineHeight="32" padding="0" margin="0" shadow="0" grid="s12">
                    <m:MaterialCollapsibleItem lineHeight="32">
                    <m:MaterialCollapsibleHeader lineHeight="32" padding="0">
                        <m:MaterialLink lineHeight="32" textColor="BLUE" iconType="HELP" iconSize="TINY" iconPosition="LEFT" text="The Menu Option" ui:field="title"></m:MaterialLink>
                    </m:MaterialCollapsibleHeader>
                    <m:MaterialCollapsibleBody lineHeight="32">
                        <m:MaterialRow lineHeight="32">
                            <m:MaterialColumn lineHeight="32" ui:field="help" grid="s12">
                            </m:MaterialColumn>
                        </m:MaterialRow>
                    </m:MaterialCollapsibleBody>
                    </m:MaterialCollapsibleItem>
                </m:MaterialCollapsible>
            </m:MaterialColumn>
        </m:MaterialRow>
        <m:MaterialRow lineHeight="32">
            <m:MaterialColumn lineHeight="32" grid="s12">
                <m:MaterialListBox lineHeight="32" ui:field="menu" old="true"></m:MaterialListBox>
            </m:MaterialColumn>
        </m:MaterialRow>
    </m:MaterialPanel>
</ui:UiBinder>